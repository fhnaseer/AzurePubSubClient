<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:PubSubClient.ViewModels"
                    xmlns:views="clr-namespace:PubSubClient.Views">
    <DataTemplate DataType="{x:Type viewModels:HomeViewModel}">
        <views:HomeView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:SubscriberViewModel}">
        <views:SubscriberView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:PublisherViewModel}">
        <views:PublisherView />
    </DataTemplate>
</ResourceDictionary>